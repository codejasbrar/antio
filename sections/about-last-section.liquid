<section id="about_last_section">
  <div class="quality_head">
    <p class="subheading red_text text-center mb-10">{{ section.settings.heading }}</p>
    <h1 class="heading text-center">{{ section.settings.subheading }}</h1>
  </div>
  <div class="quality_content_image" style="background-image: url({{ section.settings.content_image | img_url: 'master' }});">
  </div>
  <div class="content_section">
    <p>{{ section.settings.content }}</p>
    <div class="content_btn">
      <a href="{{ section.settings.btn1_link }}" class="btn btn_dark">{{ section.settings.btn1_text }}</a>
      <a href="{{ section.settings.btn2_link }}" class="btn btn_dark second_button">{{ section.settings.btn2_text }}</a>
    </div>
  </div>
</section>
{% schema %}
  
  {
    "name": "About last section",
    "settings": [

            {
              "type": "text",
              "id": "heading",
              "label": "Heading"
            },
            {
              "type": "text",
              "id": "subheading",
              "label": "Subheading"
            },
            {
              "type": "image_picker",
              "id": "content_image",
              "label": "Image"
            },
            {
              "type": "richtext",
              "id": "content",
              "label": "Content"
            },
            {
              "type": "url",
              "id": "btn1_link",
              "label": "Button 1 Url"
            },
            {
              "type": "text",
              "id": "btn1_text",
              "label": "Button 1 text"
            },
            {
              "type": "url",
              "id": "btn2_link",
              "label": "Button 2 Url"
            },
            {
              "type": "text",
              "id": "btn2_text",
              "label": "Button 1 text"
            }
        ]
  }
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

{% javascript %}
{% endjavascript %}
